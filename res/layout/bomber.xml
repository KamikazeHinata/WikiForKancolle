                android:background="@drawable/buttom_style" />

            <LinearLayout
                android:id="@+id/t_zuiun634Tp12"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignTop="@+id/zuiun634Tp12"
                android:gravity="center"
