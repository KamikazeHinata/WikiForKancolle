                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="葛城"
                    android:textColor="@color/black"
                    android:textSize="20dp" />
            </LinearLayout>
            
             <LinearLayout
                android:id="@+id/tp8"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_below="@+id/katsuragi"
                android:background="@color/lblue"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="齐柏林伯爵型"
                    android:textColor="@color/black"
                    android:textSize="20dp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/GrafZeppelin"
                android:layout_width="350sp"
                android:layout_height="50sp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/tp8"
                android:background="@drawable/buttom_style" />

            <LinearLayout
                android:id="@+id/GrafZeppelin1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignTop="@+id/GrafZeppelin"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
