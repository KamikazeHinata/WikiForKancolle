                    android:textSize="20dp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/hiryuu"
                android:layout_width="350sp"
                android:layout_height="50sp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/tp4"
